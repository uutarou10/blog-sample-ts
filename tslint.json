{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "variable-name": [true, "allow-leading-underscore"],
    "interface-name": [true, "never-prefix"]
  },
  "jsRules": {
    "object-literal-sort-keys": false
  }
}